  "inbounds": [
    {
      "type": "tun",
      "address": "172.19.0.1/30",
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "endpoint_independent_nat": false,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 7898
        }
      }
    },
    {
      "type": "mixed",
      "listen": "127.0.0.1",
      "listen_port": 7898,
      "users": []
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "DIRECT"
    },
    {
      "type": "block",
      "tag": "BLOCK"
    },
    {
      "tag": "auto",
      "type": "urltest",
      "outbounds": ["DIRECT"],
      "url": "http://www.gstatic.com/generate_204",
      "interval": "10m",
      "tolerance": 50
    },
    {
      "tag": "Proxy",
      "type": "selector",
      "outbounds": ["auto", "DIRECT"]
    },
    {
      "tag": "Global",
      "type": "selector",
      "outbounds": ["Proxy", "DIRECT"]
    },
    {
      "tag": "China",
      "type": "selector",
      "outbounds": ["DIRECT", "Proxy"]
    },
    {
      "tag": "AdBlock",
      "type": "selector",
      "outbounds": ["BLOCK", "DIRECT"]
    }
  ],
  "route": {
    "auto_detect_interface": true,
    "final": "DIRECT",
    "default_domain_resolver": "ali",
