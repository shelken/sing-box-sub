  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "access_control_allow_origin": ["http://127.0.0.1"],
      "access_control_allow_private_network": true,
      "external_ui": "ui",
      "secret": "",
      "external_ui_download_url": "https://v6.gh-proxy.org/https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "DIRECT",
      "default_mode": "rule"
    },
    "cache_file": {
      "enabled": true,
      "store_fakeip": false
    }
  },
  "ntp": {
    "enabled": true,
    "server": "time.apple.com",
    "server_port": 123,
    "interval": "30m",
    "detour": "DIRECT"
  },
  "dns": {
    "final": "ali",
    "strategy": "prefer_ipv4", //prefer_ipv4 prefer_ipv6 ipv4_only ipv6_only
    "disable_cache": false,
    "cache_capacity": 8096,
    "servers": [
      //direct
      {
        "tag": "ali",
        "type": "udp",
        "server": "223.5.5.5"
      },
      {
        "tag": "aliTls",
        "type": "https",
        "server": "223.5.5.5"
      },
      {
        "tag": "tvbox",
        "type": "udp",
        "server": "192.168.6.141"
      },
      //proxy
      {
        "tag": "googleDns",
        "type": "tls",
        "server": "8.8.8.8",
        "detour": "Proxy"
      },
      {
        "tag": "cfDns",
        "type": "tls",
        "server": "1.1.1.1",
        "detour": "Proxy"
      },
      {
        "tag": "cfHttps",
        "type": "https",
        "server": "cloudflare-dns.com",
        "detour": "Proxy",
        "domain_resolver": "cfDns"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
